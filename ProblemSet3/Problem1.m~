function Problem1()
x = 1;

y25 = series(x,.25,5);
y50 = series(x,.50,5);
y75 = series(x,.75,5);

y25 = series(x,.25,5);
y50 = series(x,.50,5);
y75 = series(x,.75,5);

end


function temp = series(x,y,iter)
L = 2;
w = 1;
t1 = 50;
t2 = 150;

theta_ = 0;
for i = 1:iter
    theta = (2/pi) * ((-1^(iter+1)+1)*sin(n*pi*x/L)*(sinh(iter*pi*y/L)/sinh(iter*pi*w/L)));
    theta_ = theta_ + theta;
end

temp = theta_*(t2-t1) + t1;
end